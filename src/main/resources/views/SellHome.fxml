<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>
<?import java.net.*?>

<HBox minHeight="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.tallymed.ui.views.SellHomeController">
	<children>
		<ScrollPane minWidth="600.0">
			<content>
				<VBox>
					<children>
						<ScrollPane>
							<content>
								<FlowPane fx:id="productFlowPane">
									<children>
                              <JFXTextField focusColor="#009900" labelFloat="true" layoutX="550.0" layoutY="15" maxWidth="170.0" minWidth="170.0" prefHeight="30.0" prefWidth="170.0" promptText="Search here.." unFocusColor="#5b5b5b">
                                 <font>
                                    <Font size="13.0" />
                                 </font></JFXTextField>
										<TreeTableView fx:id="treeView">
                                 <FlowPane.margin>
                                    <Insets top="30.0" />
                                 </FlowPane.margin></TreeTableView>
									</children>
								</FlowPane>
							</content>
						</ScrollPane>
						<AnchorPane maxHeight="200.0" minHeight="200.0" prefHeight="200.0">
                     <children>
                        <Label text="Label" />
                     </children></AnchorPane>
					</children>
				</VBox>
			</content>
		</ScrollPane>
		<AnchorPane maxWidth="200.0" minWidth="200.0" prefWidth="200.0" />
	</children>
	<stylesheets>
      <URL value="@themes/defaultTheme.css" />
      <URL value="@themes/jfxTreeView.css" />
   </stylesheets>
</HBox>
